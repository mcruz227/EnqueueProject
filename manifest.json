

{
    "manifest_version" : 3,
    "name" : "Enqueue Extension",
    "version": "1.0" ,
    "description" : "Chrome Extension improving queue and watch later features from the Youtube Platform.",
    "permissions" : [
        "activeTab",
        "storage"
    ],
   
    "action": {
        "default_popup": "enqueue.html"

    },
    "content_scripts": [
           {
             "matches": ["https://youtube.com/*"],
             "css": ["enqueue.css"],
             "js": ["enqueue.js"]

            }
        ]

    }



